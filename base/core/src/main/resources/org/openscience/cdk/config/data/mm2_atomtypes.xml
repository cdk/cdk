<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="http://cdk.svn.sourceforge.net/viewvc/*checkout*/cdk/trunk/cdk/src/org/openscience/cdk/config/data/html.xsl?content-type=text%2Fxml"?>
<atomTypeList xmlns="http://www.xml-cml.org/schema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.xml-cml.org/schema ../../io/cml/data/cml25b1.xsd"

  xmlns:cvs="https://www.cvshome.org/"
  xmlns:cdk="http://cdk.sf.net/dict/cdk/"

  id="mm2" title="MM2 AtomTypes">

  <metadataList>
    <metadata name="cvs:last-change-by" content="$Author$"/>
    <metadata name="cvs:date" content="$Date$"/>
    <metadata name="cvs:revision" content="$Revision$"/>
  </metadataList>
  
  <atomType id="C">
	<!-- for example in CC-->
    <atom elementType="C" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">4</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">4</scalar>
   </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp3</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">[CSP]-[0-4][-]?+;[A-Za-z\+\-&amp;&amp;[^=%]]{0,6}[(].*+</scalar>
  </atomType>
  <atomType id="Csp2">
   <!-- for example in C=C -->
    <atom elementType="C" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">4</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">[CS]-[0-3];[H]{0,2}+[A-Za-z]*+=[A-Z]{1,2}+.*+</scalar>
  </atomType>
  <atomType id="CdoubleBonded">
  <!-- for example in >C=0,P,S -->
    <label value="C="/>
    <atom elementType="C" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">4</scalar>
   </atom>
    <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">C-[0-3];=O.*+</scalar>
  </atomType>
 <atomType id="Csp">
  <!-- for example in >C=0 -->
    <atom elementType="C" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">3.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">2</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">4</scalar>
	</atom>
    <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp1</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">C-[1-2][-]?+;[H]{0,1}+%.*+</scalar>
  </atomType>
  <atomType id="C.">
  <!-- carboradical -->
    <atom elementType="C" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">3.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp3</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">NOT Implemented</scalar>
  </atomType>
  <atomType id="Cplus">
  <!-- carbocation -->
    <label value="C+"/>
    <atom elementType="C" formalCharge="1">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">3.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">3.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">C-[0-9][+];.*+</scalar>
  </atomType>
  <atomType id="CR3R">
   <!-- Csp3 in 3 member rings -->
    <atom elementType="C" formalCharge="0">     
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">4</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">4</scalar>
    </atom>
     <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp3</scalar>
     <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
     <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">C-3;CCC..?+&amp;?+[A-Za-z]?+,?+.?+&amp;?+,?+.?+&amp;?+.*+</scalar>
     <scalar dataType="xsd:int" dictRef="cdk:ringSize">3</scalar>
     <scalar dataType="xsd:integer" dictRef="cdk:ringConstant">3</scalar>    
   </atomType> 
   <atomType id="CE3R">
     <!-- Csp2 in 3 membered rings -->
    <atom elementType="C" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">4</scalar>
    </atom>
     <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
     <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
     <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">C-2;=CC..?+[A-Za-z]?+,?+&amp;?+,?+C?+&amp;?+.*+</scalar>
     <scalar dataType="xsd:int" dictRef="cdk:ringSize">3</scalar>
     <scalar dataType="xsd:integer" dictRef="cdk:ringConstant">3</scalar>    
   </atomType> 
   <atomType id="Car">
    <!-- for example in benzol -->
    <atom elementType="C" formalCharge="0">
     <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">4</scalar>
    </atom>
     <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
     <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
     <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">C-2;=CC.*+</scalar>
     <scalar dataType="xsd:integer" dictRef="cdk:aromaticAtom">4</scalar>
     <scalar dataType="xsd:integer" dictRef="cdk:ringConstant">3</scalar>    
   </atomType> 
   <atomType id="Cplusplus">
    <!-- ketonium -->
    <label value="C++"/>
    <atom elementType="C" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">4</scalar>
    </atom>
     <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
     <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
     <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">NotImplemented</scalar>
   </atomType> 
   <atomType id="HC">
    <!-- H attached to C -->
    <atom elementType="H" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">1</scalar>
    </atom>
    <scalar dataType="xsd:string" dictRef="cdk:DA">-</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">H-[0-1];[C].*+</scalar>
  </atomType>
  <atomType id="HS">
    <!-- H attached to sulfur -->
    <atom elementType="H" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">1</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:DA">D</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">H-1;S.*+</scalar>
  </atomType>
  <atomType id="HN">
    <!-- H attached to nitrogen -->
    <atom elementType="H" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">1</scalar>
    </atom>
    <scalar dataType="xsd:string" dictRef="cdk:DA">D</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">H-1;[NP][+]?+[(][H]{0,2}+=?+[A-Z]{0,2}+/.*+</scalar>
  </atomType>
  <atomType id="HO">
    <!-- H in alcohol -->
    <atom elementType="H" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">1</scalar>
    </atom>
    <scalar dataType="xsd:string" dictRef="cdk:DA">D</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">H-1;O[+-]?+.[PSCN]{0,2}+/.*+</scalar>
  </atomType>
  <atomType id="HOCO">
    <!-- H carboxylic groups -->
    <atom elementType="H" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">1</scalar>
    </atom>
    <scalar dataType="xsd:string" dictRef="cdk:DA">D</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">H-1;O[+]?+.=?+C/=?+[OCSP]{1,2}+/.*+</scalar>
  </atomType>
  <atomType id="HN2">
    <!-- H in amid -->
    <atom elementType="H" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">1</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:DA">D</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">H-1;N[(]{1}+[CH]{2,2}+/[H]{0,3}+[,]?+=OC.*+</scalar>
  </atomType>
   <atomType id="HOC">
    <!-- H in enol -->
    <atom elementType="H" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">1</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:DA">D</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">H-1;O.C/=CC/.*+</scalar>
  </atomType>
  <atomType id="HNplus">
    <!-- H in ammonium -->
    <label value="HN+"/>
    <atom elementType="H" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">1</scalar>
    </atom>
    <scalar dataType="xsd:string" dictRef="cdk:DA">D</scalar>
    <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">H-1;N[+].[A-Z]{0,3}+/.*+</scalar>
  </atomType>
  <atomType id="O">
    <!-- for example in alcohol or ether, or sulfur as thiol -->
    <atom elementType="O" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">2.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">2</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">6</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp3</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">[OS]-[0-2][-]?+;[A-Za-z]{1,4}+[+]?+[(].*+</scalar>
  </atomType>
  <atomType id="OdoubleBonded">
    <!-- carbonyl oxygen double bonded -->
    <label value="O="/>
    <atom elementType="O" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">2.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">6</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">O-[1-2][+]?+;[H]{0,1}+=[SPC].[^O]++.*+</scalar>
  </atomType>
  <atomType id="OX">
    <!-- amine oxide -->
    <atom elementType="O" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">2.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">6</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">O-1[-]?+;=?+N.*+</scalar>
  </atomType>  
  <atomType id="OK">
    <!-- ketoniumoxide -->
    <atom elementType="O" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">6</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp3</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">O-3[+];[H]{0,3}+[C]{0,3}+[(].*+</scalar>
  </atomType>   
 <atomType id="OM">
    <!-- carboxylate -->
    <atom elementType="O" formalCharge="-1">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">1.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">1</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">6</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp3</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">O-1-?+;=?+[CS][(][=0]?+[OCSNH]*+/.*+</scalar>
  </atomType>  
  <atomType id="OR">
    <!-- epoxy -->
    <atom elementType="O" formalCharge="1">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">2.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">2</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">6</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">O-2;CC.H?+,?+H?+,?+&amp;,&amp;.*+</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:ringSize">3</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:ringConstant">0</scalar> 
  </atomType>  
  <atomType id="Oar">
    <!-- for example in furan -->
    <atom elementType="O" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">2.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">2</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">6</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">O-2;CC.=C.*+&amp;amp;.*+&amp;amp;.*+</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:ringSize">5</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:aromaticAtom">4</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:ringConstant">6</scalar> 
  </atomType> 
  <atomType id="N">
    <!-- in aliphatic amines -->
    <atom elementType="N" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">1.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">3.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">3</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">5</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp3</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">N-[0-3][+-]?+;[A-Z &amp;&amp;[^=%]]{1,3}+.*+</scalar>
  </atomType>
  <atomType id="Nsp2">
    <!-- in imines -->
    <atom elementType="N" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">3.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">2</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">5</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">N-[1-3][-+]?+;[^%]=?+[ON]?+[+]?+[CH]*+.(=O)?+.*+</scalar>
  </atomType>
  <atomType id="Namid">
    <!--  -->
    <atom elementType="N" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">3.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">2</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">5</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp2</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">N-[1-3];[CH]{1,3}.{1}+[A-Z]{0,3}+[,]?+=OC.*+</scalar>
  </atomType>
  <atomType id="Nsp">
    <!-- triplebonded -->
    <atom elementType="N" formalCharge="0">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">3.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">2</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">5</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp1</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">N-[1-2][+]?+;%.*+</scalar>
  </atomType>
  <atomType id="NdoubleDoubleBonded">
    <!-- central nitrogen for example in C=N=N or N=N=N -->
    <label value="=N="/>
    <atom elementType="N" formalCharge="1">
      <scalar dataType="xsd:double" dictRef="cdk:maxBondOrder">2.0</scalar>
      <scalar dataType="xsd:double" dictRef="cdk:bondOrderSum">4.0</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:formalNeighbourCount">2</scalar>
      <scalar dataType="xsd:integer" dictRef="cdk:valency">5</scalar>
    </atom>
      <scalar dataType="xsd:string" dictRef="cdk:hybridization">sp1</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:DA">A</scalar>
      <scalar dataType="xsd:string" dictRef="cdk:sphericalMatcher">N-2[+];=?+%?+[NC][-=]{0,2}+[NC][-]?+.*+</scalar>
  </atomType>
  <atomType id="Nplus">
    <!-- for example in ammonium -->
    <label value="N+"/>
    <atom elementType="N" formalCharge="1">
